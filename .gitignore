# Ignore bundler config.
/.bundle
# Ignore all logfiles and tempfiles.
/log/*
/docs/rdoc/*
/coverage/*
/tmp/*
#!/tmp/pids
!/tmp/pids/.keep
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history
.ruby-version
.ruby-gemset
.env_deployment

/docker-compose.*custom*.yml
vendor/bundle

# Ignore uploaded files in development
/storage/*
!/storage/.keep

# Fits
fits.log

/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity
